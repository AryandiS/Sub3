{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "quotes": "off",
    "no-underscore-dangle": "off",
    "no-restricted-globals": "off",
    "no-console": "off",
    "indent": "off",
    "comma-dangle": "off"
  }
}
